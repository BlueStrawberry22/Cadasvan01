@model IEnumerable<Cadasvan01.Models.Usuario>

@{
    ViewData["Title"] = "Alunos";
}

<h1>Alunos</h1>

<table class="table">
    <thead>
        <tr>
            <th>Nome Completo</th>
            <th>Endereço</th>
            <th>Cidade</th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        @foreach (var user in Model)
        {
                <tr>
                    <td>@user.NomeCompleto</td>
                    <td>@user.Endereco</td>
                    <td>@user.Cidade</td>
                    <!--Tenho que fazer a cidade aparecer de alguma forma.-->
                </tr>
        }
    </tbody>
</table>