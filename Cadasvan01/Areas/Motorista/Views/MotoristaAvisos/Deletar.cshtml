@model Cadasvan01.Models.Aviso

<h2>Apagar Aviso</h2>

<form asp-action="DeletarConfirma" method="post">
    <div class="form-group">
        <input type="hidden" asp-for="AvisoId" />
        <label>Titulo:</label>
        <input asp-for="Titulo" class="form-control" disabled />
    </div>
    <div class="form-group">
        <label>Conteudo:</label>
        <textarea asp-for="Conteudo" class="form-control" disabled></textarea>
    </div>
    <button type="submit" class="btn btn-danger">Apagar</button>
</form>
