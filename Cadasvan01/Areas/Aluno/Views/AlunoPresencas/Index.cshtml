@model IEnumerable<Cadasvan01.Models.Presenca>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DataViagem)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConfirmadoIda)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConfirmadoVolta)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DataViagem)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ConfirmadoIda)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ConfirmadoVolta)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.PresencaId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.PresencaId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.PresencaId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
